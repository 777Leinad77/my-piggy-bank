<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.projetbudget">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ProjetBudget">
        <activity
            android:name=".projet.ProjetInfo"
            android:exported="false" />
        <activity
            android:name=".projet.ProjetActivity"
            android:exported="false" />
        <activity
            android:name=".projet.CreationProject"
            android:exported="false" />
        <activity
            android:name=".activity.GestionDepense"
            android:exported="true"
            android:theme="@style/Theme.ProjetBudget.NoActionBar" />
        <activity
            android:name=".activity.GestionProjet"
            android:exported="true"
            android:theme="@style/Theme.ProjetBudget.NoActionBar" />
        <activity
            android:name=".activity.MainActivity"
            android:exported="true"
            android:theme="@style/Theme.ProjetBudget.NoActionBar" />
        <activity
            android:name=".activity.StartActivity"
            android:exported="true"
            android:theme="@style/Theme.ProjetBudget.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>